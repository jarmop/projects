# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET        /                                    controllers.Application.index
GET        /tax/:country                        controllers.Application.tax(country: String, earnedIncome: Double, municipality: String, age: Int)

GET        /compare                             controllers.Compare.index
GET        /compare/update                      controllers.Compare.updateAll

GET        /compare/fi/percent                  controllers.Compare.fiPercent(update: Boolean ?= false)
GET        /compare/fi/percent/update           controllers.Compare.fiPercent(update: Boolean ?= true)
GET        /compare/fi/sum                      controllers.Compare.fiSum(update: Boolean ?= false)
GET        /compare/fi/sum/update               controllers.Compare.fiSum(update: Boolean ?= true)
GET        /compare/fi/net-income               controllers.Compare.fiNetIncome(update: Boolean ?= false)
GET        /compare/fi/net-income/update        controllers.Compare.fiNetIncome(update: Boolean ?= true)

GET        /compare/sv/percent                  controllers.Compare.svPercent(update: Boolean ?= false)
GET        /compare/sv/percent/update           controllers.Compare.svPercent(update: Boolean ?= true)
GET        /compare/sv/net-income               controllers.Compare.svNetIncome(update: Boolean ?= false)
GET        /compare/sv/net-income/update        controllers.Compare.svNetIncome(update: Boolean ?= true)
GET        /compare/sv/sum                      controllers.Compare.svSum(update: Boolean ?= false)
GET        /compare/sv/sum/update               controllers.Compare.svSum(update: Boolean ?= true)

GET        /compare/de/percent                  controllers.Compare.dePercent(update: Boolean ?= false)
GET        /compare/de/percent/update           controllers.Compare.dePercent(update: Boolean ?= true)
GET        /compare/de/net-income               controllers.Compare.deNetIncome(update: Boolean ?= false)
GET        /compare/de/net-income/update        controllers.Compare.deNetIncome(update: Boolean ?= true)
GET        /compare/de/sum                      controllers.Compare.deSum(update: Boolean ?= false)
GET        /compare/de/sum/update               controllers.Compare.deSum(update: Boolean ?= true)


GET        /compare/percent                     controllers.Compare.percent(update: Boolean ?= false)
GET        /compare/percent/update              controllers.Compare.percent(update: Boolean ?= true)
GET        /compare/net-income                  controllers.Compare.netIncome(update: Boolean ?= false)
GET        /compare/net-income/update           controllers.Compare.netIncome(update: Boolean ?= true)

GET        /mongo                               controllers.MongoTest.index
GET        /mongo/create                        controllers.MongoTest.create(name: String = "jarmo", age: Int = 30)

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                        controllers.Assets.versioned(path="/public", file: Asset)

GET        /webjars/*file                       controllers.WebJarAssets.at(file)